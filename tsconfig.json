{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        //"outDir": "built",
        "rootDir": "./",
        "noImplicitAny": false,
        "jsx": "react",
        "strict": false,
        "allowJs": false,
        "noImplicitUseStrict": true
    },
    "typeAcquisition": {
        "enable": true
    },
    "exclude": [
        "config",
        "node_modules",
        "public",
        "scripts",
        "logs",
        "build",
        "data"
    ]
}